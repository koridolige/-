        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.oceanbioserver.mapper.CategoryMapper">

    <resultMap id="BaseResultMap" type="com.example.oceanbioserver.entity.Category">
